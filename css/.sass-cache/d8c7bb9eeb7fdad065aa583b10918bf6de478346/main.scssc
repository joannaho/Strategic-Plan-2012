/* Goal Specific */
#goal1 .goal-intro {
	background:$goal1color;
}
#goal2 .goal-intro {
	background:$goal2color;
}
#goal3 .goal-intro {
	background:$goal3color url('../images/goal3-sprite.png') no-repeat scroll 110% 50px;
}
#goal4 .goal-intro {
	background:$goal4color url('../images/goal4-bg.jpg') no-repeat scroll 50% 50%;
}



