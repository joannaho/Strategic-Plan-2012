	z-index:1;
}
.elements.sphere2 { 
	background-position:-438px -5px; 
	width:300px; 
	height:300px; 
	top:400px;
	left:-50px; 
}
.elements.sphere3 { 
	background-position:-761px -5px; 
	width:200px; 
	height:200px; 
	top:-190px;
	right:0; 
}
.elements.sphere4 { 
	background-position:-783px -226px;
	width:176px; 
	height:176px; 
	top:-100px;
	left:-30px; 
}
.elements.sphere5 { 
	background-position:-970px top;
	width:500px; 
	height:500px; 
	left:87%;
	top:130px;
	z-index:3;
}	

/* -----------------------------------------
   Goals Detail
----------------------------------------- */
dl.tabs dd a { 
	border:1px solid #ccc;
	color:#444;
	padding:0 15px;
	&.active { 
		color:#226ABC;
		font-weight:bold;
	}
}

.goal-intro {
	position:relative;
	min-height:400px;
	max-height:400px;
	margin-bottom:1.5em;
	color:#fff;
	overflow:hidden;
	.row { 
		position:relative;
		top:240px;
		z-index:999;
		@include transition(top ,1s, linear);
		.goalNum {
			line-height:1.25em;
			font-size:2.5em;
			font-weight:200;
			text-align:center;
			.num {
				display:block;
				font-family: 'Times';
				font-size:3em;
				line-height:1em;
			}
		}
		.goalHd { 
			h2 { color:#fff; line-height:1em; }
			p { font-size:1.35em; }
		}
	}
}

.goal-content { 
	.four { padding-top:2em; 
		img { 
			max-width:100%;
		}	
	}
	.extras { 
		margin:20px 0 0 30px;
	}
}

dl.tabs { 
	margin:2.5em 0 1.5em;
}
.tabs-content { 
	clear:both;
	h6 { margin-left:15px; display:none; }
	li { margin-bottom:0;
		li { 
			list-style:disc;
			margin-bottom:12px;
		}
	}
}
.progress { 
	margin:4em 0 2em;
}
.progress-heading { 
	text-transform:uppercase;
	text-align:center;
	display:block;
	margin:0;
	border-top:1px solid #ccc;
	color:#555;
	span { 
		background:#fff;
		font-size:1.5em;
		font-weight:200;
		position:relative;
		top:-13px;
		padding:0 15px;
	}
}
.progress-block {
	float:left;
	background:#eee;
	border-radius: 5px;
	text-align:center;
	vertical-align:middle;
	min-height:200px;
	background-repeat:no-repeat;
	background-position:top right;
	&:first { margin-left:none; }
	.num { 
		display:block; 
		color:#5194E2;
		text-shadow:0 1px 0px #fff;
		padding:0.4em 0 0.3em;
		font-size:4em;
		&.small { font-size:3em; line-height:1.65em; }
		span { 
			display:block;
			font-size:0.4em;
			color:#333;
			margin-top:-5px;
		}
	}
	.desc { 
		display:block;
		text-shadow:0 1px 0px #fff;
		border-top:1px solid #fff;
		box-shadow:0 -1px 0 #ccc; 
		padding:1em; 
		font-size:0.95em;
	}
	&.jobs { 
		background-image:url(../images/box-bg-networking.jpg); 
		background-position:55px -17px; 
	}
	&.nobel { 
		background-image:url(../images/box-bg-noble.jpg); 
		background-position:-8px 7px;
		.num { 
			text-align:left;
			padding-left:0.9em; 
		}
	}
	&.flag { 
		background-image:url(../images/box-bg-flag.jpg); 
		background-position:33px -3px; 
	}
	&.capital { 
		background-image:url(../images/box-bg-capital.jpg); 
		background-position:67px -28px; 
	}
}

.extras a { 
	background: #555555; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #555555 0%, #222222 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#555555), color-stop(100%,#222222)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #555555 0%,#222222 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #555555 0%,#222222 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #555555 0%,#222222 100%); /* IE10+ */
	background: linear-gradient(top,  #555555 0%,#222222 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#555555', endColorstr='#222222',GradientType=0 ); /* IE6-8 */
	border-radius:50px;
	width:36px;
	height:36px;
	display:inline-block;
	margin-right:5px;
	cursor:pointer;
	span { 
		background:transparent url('../images/sprite.png') no-repeat scroll 100px 100px; 
		width:24px;
		height:24px;
		display:block;
		margin:6px auto 0;
	}
	&.video span { background-position:-1px -33px; }
	&.charts span { background-position:2px -65px; }
	&.chat span { background-position:1px -96px; }
}
.extras a:hover { 
	background: #5195e0; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #5195e0 0%, #11396a 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#5195e0), color-stop(100%,#11396a)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #5195e0 0%,#11396a 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #5195e0 0%,#11396a 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #5195e0 0%,#11396a 100%); /* IE10+ */
	background: linear-gradient(top,  #5195e0 0%,#11396a 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#5195e0', endColorstr='#11396a',GradientType=0 ); /* IE6-8 */
}
.video-box {
	a { display:-moz-stack; display:block;
		object { width:480x; height:360px; display:block; }
	}
}


/* Goal Specific */
#goal1 .goal-intro {
	background-color:$goal1color;
	.bg { 
		position:absolute;
		//background-image:url('../images/goal1-bg.jpg');
		background-repeat:no-repeat;
		background-position:75% 20px;
		background-attachment:fixed;
		width:100%;
		height:401px;
		display:none;	
	}
	#parallax { 
		position:absolute; 
		right:0;
		bottom:0;
		//overflow:hidden;
		width:40%;
		height:400px;
	}
	.sprite { 
		position:absolute;
		background-color:transparent;
		//background-image:url(../images/goal1-sprite.png);
		background-repeat:no-repeat;
		background-position:0 0;	
		display:block;
		display:none;
		&.layer1 { 
			background-image:url(../images/goal1-sprite1.png) !important;
			width:727px;
			height:750px;
			//background-position:right bottom; 
			//top:150px;
			top:90% !important;
			right:-460px;
			z-index:3
		}
		&.layer2 { 
			width:500px;
			height:565px;
			background-position:-11px -24px; 
			right:-40px;
			z-index:2;
		}
		&.layer3 { 
			width:626px;
			height:760px;
			background-position:-515px -1px; 
			top:-90px;
			right:-90px !important;
			z-index:1;
		}
	}		
}
#goal2 .goal-intro {
	background:#111;
	.sprite-wrap { 
		position:relative;
		top:-137px;
		margin:0 auto;
		width:1280px;
		height:400px;
		.sprite { 
			//background-image:url('../images/goal2-bg.jpg');
			background-repeat:no-repeat;
			background-position:0 0;
			width:320px;
			height:200px;
			position:absolute;
			float:left;
			opacity:0;
			filter:alpha(opacity = 15);
			&.img1 { background-position:0 top; top:0px; left:0px; }
			&.img2 { background-position:-320px top; top:0px; left:320px;}
			&.img3 { background-position:-640px top; top:0px; left:640px;}
			&.img4 { background-position:-960px top; top:0px; left:960px;}
			&.img5 { background-position:0 bottom; bottom:0px; left:0px;}
			&.img6 { background-position:-320px bottom; bottom:0px; left:320px;}
			&.img7 { background-position:-640px bottom; bottom:0px; left:640px;}
			&.img8 { background-position:right bottom; bottom:0px; right:0px;}
		}
	}
}
#goal3 .goal-intro {
	background-color:$goal3color;
	.sprite { 
		//background-image:url('../images/goal3-sprite.png');
		background-repeat:no-repeat;
		background-position:0 0;
		@include transition(right ,1s, linear);
		display:none;
		width:528px;
		height:349px;
		position:absolute;
		bottom:0px;
		right:-3%;
	}
}
#goal4 .goal-intro {
	background-color:$goal4intro;
	.row { 
	}
	.sprite { 
		background-color:transparent;
		background-repeat:no-repeat;
		display:none;
		@include transition(right ,1s, linear);
		
		&.bg { 
			//background-image:url('../images/goal4-bg.jpg');
			background-position:50% -80%;
			background-attachment:fixed;
			width:100%;
			height:761px;
			position:absolute;
		}
		&.element1 { 
			//background-image:url(../images/goal4-sprite.png);
			background-position:130% 0px;
			//background-attachment:fixed;
			width:581px;
			height:604px;
			top:-75px;
			right:-15%;
			position:absolute;
			display:none;
		}
		
	}
}


.preload { display:none; }


/*--------------------------------------------------
:: Media Queries
-------------------------------------------------- */
@media only screen and (max-width: 900px) {
	.goal-intro .row { top:210px; }
	#goal3 .goal-intro {
		.sprite { 
			right:-20%;
		}
	}
	#goal4 .goal-intro {
		.sprite { 
			&.element1 { 
				right:-35%;
			}
		}
	}
}

@media only screen and (max-width: 767px) {
	.block-grid.mobile.four-up li { float:left; width:48%; margin-left:2%; }
	
	header { 
		background-position:0 0; 
		min-height:45px; 
		.hd { 
			.acslogo { display:none; }
			.hd-corner { display:none; }
			.acstext { display:inline-block; float:left; margin-top:7px; }
		}
		nav { 
			top:1px; 
			width:auto;
			ul { display:none; }
			select { display:inline-block; }
		}
	}
	.container { 
		background:#C8DFEA url("../images/home-bg.gif") repeat-x scroll left top;
	}
	#visionmission { 
		background:none;
		height:auto;
		padding-top:5em;
		.title { 
			font-size:3.5em;
			line-height:1em;
			top:0;
			.sp12 { 
				left:0px;
			}
			.andbeyond {
				color:$acsdarkblue; 
				position:static;
				top:0;
				left:0;
			}
		}
		.vision, .mission { 
			top:30px;
			left:0;
			width:100%;
		}
	}
	
	#corevalue { 
		margin-top:4em;
		height:auto;
		text-shadow:none;
		color:#333 !important;
		p {
			top:0; left:0;
			width:100%;
			font-size:115%;
		}
		h1 { 
			font-size:2.5em;
			font-weight:200;
			text-align:left !important;
			left:0;
			top:0;
			width:100%;
			color:$acsdarkblue;
		}
		h5 { 
			line-height:1.2em; 
			color:#000;
		}
		.col-l, .col-r { 
			top:0;
			width:100%; 
		}
		.col-l { 
			float:left;
			left:0;
		}
		.col-r {
			float:left; 
			left:0;
		}
		ul { margin-bottom:1.5em;
			li {  }
		}
		.graphic-earth { display:none; }
		.sprite { display:none; }
	} 
		
	#goals { 
		margin-top:3em;
		padding:0;
		height:1480px;
		.preamble { 
			position:relative;
			top:0;
			left:0;
			float:none;
			width:100%; 
			h1 { 
				color:$acsdarkblue;
				font-size:2.5em; 
				font-weight:200;
			}
			span { font-size:1.5em; }
		}
		.hexwrap { float:none; }	
		.hex {
			position:relative; left:-30px;
			h4{ 
				color:#fff;
				line-height:1em;
				font-weight:bold;
				padding-top:1.75em;
				padding-bottom:0.75em;
				*padding-bottom:0;
				margin:auto;
			}
			&.one { right:0; }
			&.two { top:-22px; right:0; }
			&.three { top:-47px; right:0; }
			&.four { top:-71px; right:0; }
		}
		.sprite-wrap { 
			.elements { display:none !important; }
		}
	}
	.elements, .sprite { display:none !important; }
	
	.goal-intro {
		background-image:none !important;
		max-height:350px !important;
		min-height:350px !important;
		.bg { background-image:none !important; }
		.row { 
			top:50px !important;
			.goalNum { 
				margin-bottom:0.5em;
				font-size:200% !important;
				.num { font-size:450% !important; }
			}
			.goalHd { 
				text-align:center; 
				h2 { font-size:200% !important; }
				p { font-size:125% !important; }
			}
		}
	}
	dl.tabs { margin-bottom:3em !important; }
	.tabs-content { font-size:1.15em; }
	.goal-content { 
		.four { text-align:center; padding-bottom:2em; }
	}
	.progress { margin:1.25em 0 2em; }
	.progress-block { min-height:260px;
		.desc { font-size:1.15em; }
	}
	
	.reveal-modal { top:0.5%; }
}


@media only screen and (max-width: 320px) {
	.progress-block { 
		min-height:220px;
		.num { font-size:2.8em; 
			&.small { font-size:2.25em; }
		}
		.desc { font-size:1em; }
		&.job { background-position:8px -32px; }
		&.nobel { background-position:-40px -16px; }
		&.flag { background-position:-7px 0; }
		&.capital { background-position:24px -26px; }
	}
