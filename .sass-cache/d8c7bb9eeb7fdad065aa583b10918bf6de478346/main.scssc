@mixin gradient-linear($from, $to) { 
	background-color: $to;
  	background: -moz-linear-gradient($from, $to);
  	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from($from), to($to));
  	background: -webkit-linear-gradient($from, $to);
  	background: -o-linear-gradient($from, $to);	
	background: -ms-linear-gradient(top, $from 0%,$to 100%);
	background: linear-gradient(top, $from 0%,$to 100%);
	//filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='$from', endColorstr='$to',GradientType=0 );
	//zoom: 1;      
}
@mixin gradient-radial($to, $from) { 
	background: $to;
	background: -moz-radial-gradient(center, ellipse cover,  $from 25%, $to 70%);
	background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(25%,$from), color-stop(70%,$to));
	background: -webkit-radial-gradient(center, ellipse cover,  $from 25%,$to 70%);
	background: -o-radial-gradient(center, ellipse cover, $from 25%,$to 70%);
	background: -ms-radial-gradient(center, ellipse cover, $from 25%,$to 70%);
	background: radial-gradient(center, ellipse cover, $from 25%,$to 70%);
	//filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='$from', endColorstr='$to',GradientType=1 );
	//zoom: 1;      
}

@mixin transition($property, $time, $easing) {
	-webkit-transition:$property $time $easing;
	-moz-transition:$property $time $easing;
	-o-transition:$property $time $easing;
	-ms-transition:$property $time $easing;	
	transition:$property $time $easing;
}

@mixin transform($property, $deg){
	-webkit-transform: $property($deg); 
	-moz-transform: $property($deg); 
	-o-transform: $property($deg);  
	-ms-transform: $property($deg); 			
}


@-webkit-keyframes windmillAnimation {
	from { -webkit-transform: rotate(0deg);	}
	to {-webkit-transform: rotate(1440deg);	}
}
@-moz-keyframes windmillAnimation {
	from {-moz-transform: rotate(0deg); }
	to {-moz-transform: rotate(360deg); }
}
@-ms-keyframes windmillAnimation {
	from { -ms-transform: rotate(0deg); }
	to { -ms-transform: rotate(360deg); }
}

@-webkit-keyframes balloonAnimation {
	from { -webkit-transition:top 1s ease; -webkit-transition:left 1s ease; top:98px; left:680px;}
	to { -webkit-transition:top 1s ease; -webkit-transition:left 1s ease; top:76px; left:700px;}
}
@-moz-keyframes balloonAnimation {
	from { -moz-transition: top 1s ease; -moz-transition: left 1s ease; top:98px; left:680px;}
	to { -moz-transition:top 1s ease; -moz-transition:left 1s ease; top:76px; left:700px;}
}
@-ms-keyframes balloonAnimation {
	from { -ms-transition:top 1s ease; -ms-transition:left 1s ease; top:98px; left:680px;}
	to { -ms-transition:top 1s ease; -ms-transition:left 1s ease; top:76px; left:700px;}
}
@-o-keyframes balloonAnimation {
	from {
		-o-transition:top 1s ease; -o-transition:left 1s ease; top:98px; left:680px;}
	to { -o-transition:top 1s ease; -o-transition:left 1s ease; top:76px; left:700px;}
}


