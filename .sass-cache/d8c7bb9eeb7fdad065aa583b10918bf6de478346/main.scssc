/* -----------------------------------------
   Header
----------------------------------------- */
header { 
	position:fixed;
	top:0;
	width:100%;
	z-index:9998;
	background:url(../images/nav-bg.gif) repeat-x scroll 0 10px;
}

.hd { 
	position:relative;
	z-index:9999;
	.row:after { 
		clear:none;
	}
	.acslogo { 
	    position:absolute;
	    z-index:9999;
		top:14px;
		left:16px;
		background:transparent url('../images/acs-logo.png') no-repeat scroll 0 0;
		width:157px;
		height:54px;
	}
	.acstext { font-weight:bold; display:none; }
	.hd-corner { 
		width: 195px; 
		height: 60px; 
		background: #fff; 
		position: relative; 
		display:inline-block;
		float:left;
	    z-index:9999;
	} 
	.hd-corner:before { 
		content: ""; 
		position: absolute; 
		top: -25px; 
		left: 0; 
		width: 0; 
		height: 0; 
		border-left: 50px solid transparent; 
		border-right: 50px solid transparent; 
		border-bottom: 25px solid #fff; 
	} 
	.hd-corner:after { 
		content: ""; 
		position: absolute; 
		bottom: -50px; 
		left: 0; 
		width: 0; 
		height: 0; 
		border-left: 98px solid transparent; 
		border-right: 98px solid transparent; 
		border-top: 50px solid #fff; 
	}	
}

nav { 
	position:relative;
	top:-50px;
	font-size:1.25em;
	text-transform:uppercase;
	text-align:right;
	margin:0;
	width:100%;
	float:right;
	z-index:9997;
	a { 
		color:#fff;
		font-weight:500;
	}
	ul { 
		margin:0;
		display:inline-block;
		text-align:right;
		float:right;
		li { 
			float:left;
			margin:0;
			display:inline;
			cursor:pointer;
			a { 
				float:left;
				padding:8px 15px 7px;
				text-align:center;
				text-shadow:0 1px 1px #7D5200;
				&:hover { color:#fff; text-shadow:0 1px 2px #000000; }
			}
			&.current, &:hover {
				background-image:url('../images/nav-bg-on.gif');
				background-repeat:repeat-x;
				a { text-shadow:0 1px 2px #000000; }
			}
			&:hover { 
				background-position:0 -35px; 
			}
			&.current { 
				background-position:0 -70px; 
				border-left:1px solid #e0c472;
				border-right:1px solid #e0c472;
				&:hover { background-position:0 -70px; }
			}
			&.g1, &.g2, &.g3, &.g4 { 
				padding:0; margin-right:5px; 
				&.current { 
					border:none; 
					a { 
						font-size:1.5em; padding:2px 8px 11px 8px; 
					}
				}
				a { 
					display:inline-block;
					height:19px;				
					padding:7px 10px;
					border:none;
					background:#666 url('../images/nav-bg-circle.png') no-repeat scroll 0 0;
					&:hover { color:#fff; }
				}
			}
			&.g1 a, &.g2 a, &.g3 a, &.g4 a {
			}
			&.g1 a:hover, &.g1.current a { background-color:#5e437a; }
			&.g2 a:hover, &.g2.current a { background-color:#373737; }
			&.g3 a:hover, &.g3.current a { background-color:#9ec2c5; }
			&.g4 a:hover, &.g4.current a { background-color:$goal4color; }
			&.gmain, &.core { border-left:1px solid #E0C472; }
		}
	}
	select { display:none; margin-top:5px; width:100px; }
}

